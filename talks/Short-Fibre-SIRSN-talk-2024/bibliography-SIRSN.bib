title = {{A piecewise deterministic scaling limit of lifted Metropolis-Hastings in the Curie-Weiss model}},
volume = {27},
year = {2017}
}
@article{LeeJung-2018,
abstract = {Cities consist of infrastructure that enables transportation, which can be considered as topology in abstract terms. Once cities are physically organized in terms of infrastructure, people interact with each other to form the values, which can be regarded as the meta- information of the cities. The topology and meta-information coevolve together as the cities are developed. In this study, we investigate the relationship between the topology and meta-information for a street network, which has aspects of both a complex network and planar graph. The degree of organization of a street structure determines the efficiency and productivity of the city in that they act as blood vessels to transport people, goods, and information. We analyze the topological aspect of a street network using centrali- ties including the betweenness, closeness, straightness, and information. We classify the cities into several groups that share common meta-information based on the centrality, indicating that the topological factor of the street structure is closely related to meta- information through coevolution. We also obtain the coevolution in the planned cities using the regularity. Another footprint is the relation between the street segment length and the population, which shows the sublinear scaling.},
author = {Lee, Byoung-Hwa and Jung, Woo-Sung},
doi = {10.1016/j.physa.2017.12.131},
file = {:home/wilfrid/Reading/Papers/L/LeeJung-2018.pdf:pdf},
issn = {03784371},
journal = {Physica A: Statistical Mechanics and its Applications},
month = {may},
pages = {15--25},
title = {{Analysis on the urban street network of Korea: Connections between topology and meta-information}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S0378437117313808},
volume = {497},
year = {2018}
}
@article{GartnerMolchanov-1990,
abstract = {The objective of this paper is a mathematically rigorous investigation of intermittency and related questions intensively studied in different areas of physics, in particular in hydrodynamics. On a qualitative level, intermittent random fields are distinguished by the appearance of sparsely distributed sharp "peaks" which give the main contribution to the formation of the statistical moments. The paper deals with the Cauchy problem (∂/∂t)u(t, x)=Hu(t, x), u(0, x)=t0(x) ≥ 0, (t, x) ∈ ℝ+ × ℤd, for the Anderson Hamiltonian H = $\kappa$$\Delta$ + $\xi$({\textperiodcentered}), $\xi$(x), x ∈ ℤd where is a (generally unbounded) spatially homogeneous random potential. This first part is devoted to some basic problems. Using percolation arguments, a complete answer to the question of existence and uniqueness for the Cauchy problem in the class of all nonnegative solutions is given in the case of i.i.d. random variables. Necessary and sufficient conditions for intermittency of the fields u(t,{\textperiodcentered}) as t→∞ are found in spectral terms of H. Rough asymptotic formulas for the statistical moments and the almost sure behavior of u(t,x) as t→∞ are also derived. {\textcopyright} 1990 Springer-Verlag.},
author = {G{\"{a}}rtner, J and Molchanov, Stanislav A},
doi = {10.1007/BF02156540},
file = {:home/wilfrid/Reading/Papers/G/GartnerMolchanov-1990.pdf:pdf},
