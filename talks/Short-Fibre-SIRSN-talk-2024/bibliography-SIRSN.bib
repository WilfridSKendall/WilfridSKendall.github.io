@article{Kendall-2019,
abstract = {This paper reports a study of scale-invariant Rayleigh Random Flights ("RRF'') in random environments given by planar Scale-Invariant Random Spatial Networks ("SIRS") based on speed-marked Poisson line processes. RRF can be viewed as producing "randomly broken {\$}\Pi{\$}-geodesics" on the SIRSN; the aim of the study is to shed some light on whether a (non-broken) {\$}\Pi{\$}-geodesic on a SIRSN might ever come to a complete stop en route. Suitable RRF with scale-invariant dynamics ("SIRSN-RRF"), are introduced via a theory of abstract scattering representations for Markov chains (which should be of independent interest). Palm conditioning (specifically the Mecke-Slivnyak theorem for Palm probabilities of Poisson point processes) and ideas from the ergodic theory of random walks in random environments are used to show that there exists a natural scale-invariant SIRSN-RRF whose speed neither diverges to infinity nor tends to zero. This supports the conjecture that (non-broken) geodesics in Poisson line SIRSN never come to a complete stop en route, which would imply that all such {\$}\Pi{\$}-geodesics can be represented as doubly-infinite sequences of connected line segments.},
annote = {From Mathematical Reviews:
The author continues his study of scale-invariant random spatial networks (SIRSNs), as introduced by D. J. Aldous and K. Ganesan [Proc. Natl. Acad. Sci. USA 110 (2013), no. 22, 8782–8785; MR3082274] and Aldous [Electron. J. Probab. 19 (2014), no. 15; MR3164768]. The particular model of interest is built from a marked Poisson line process on the plane R2 with intensity measure given by 
$\nu$(dvdrd$\theta$)=$\gamma$−12v−$\gamma$dvdrd$\theta$,
where r∈R is the signed distance of a line from the origin, $\theta$∈[0,$\pi$) is the angle made by the line and the x-axis, v∈(0,∞) is a `speed-limit' for the line, and $\gamma${\textgreater}2 is a parameter. (Although not corresponding to a SIRSN, the borderline case $\gamma$=2 is also considered within the article.) 
   The focus of the present work is to define and explore basic properties of a natural, scale-invariant Rayleigh random flight (RRF) that has such a SIRSN as its state space. Roughly speaking, the process constructed starts by following a line in some direction at its maximum velocity, v say. On meeting a line of speed v′, it will switch to this with probability min{\{}1,(v′/v)$\alpha${\}}, where $\alpha$∈($\gamma$−1,∞) is a parameter. That is, the process will always switch to a faster line, but to a slower line with a probability depending on how its speed relates to the current speed. On transferring to a new line, a direction is chosen uniformly at random. (NB: Taking $\alpha$ within the given range ensures a certain non-degeneracy of the process.) Following the rigorous introduction of the SIRSN-RRF, a stationary measure is found for the relative environment seen from the process at times when it changes lines. To quote from the introduction of the article, this consists of `the independent superposition of a unit-speed line along the x-axis with a line through [the origin] of 
(a)
a random log-speed given by a possibly asymmetric Laplace density; 
(b)
and a random angle ϕ with the unit-speed line (with ϕ having sine-weighted density); 
(c)
together with a copy of [the SIRSN].' 

   By checking that, when started from this stationary measure, the relative-environment process is ergodic, the author concludes that the SIRSN-RRF is `speed-neighbourhood-recurrent' at the critical value $\alpha$=2($\gamma$−1) (which is the unique value for which the measure described in part (a) above is symmetric). He describes the latter result as the main contribution of the paper and asserts that it is evidence in favour of the conjecture for planar SIRSNs that geodesics between two points never come to a complete halt (away from the endpoints). To complete the article, various interesting questions that point to directions for future research on the model are raised. Reviewed by David A. Croydon},
archivePrefix = {arXiv},
arxivId = {1908.08481},
author = {Kendall, Wilfrid Stephen},
doi = {10.1214/20-EJP526},
eprint = {1908.08481},
file = {:home/wilfrid/Reading/Papers/K/Kendall-2019.pdf:pdf},
issn = {1083-6489},
journal = {Electronic Journal of Probability},
keywords = {SIRSN,SIRSN-RRF,WSK},
mendeley-tags = {SIRSN,SIRSN-RRF,WSK},
number = {124},
pages = {36pp},
title = {{Rayleigh Random Flights on the Poisson line SIRSN}},
url = {http://arxiv.org/abs/1908.08481 https://projecteuclid.org/euclid.ejp/1602489722},
volume = {25},
year = {2020}
}
