@article{DalyJohnson-2018,
abstract = {It is well known that assumptions of monotonicity in size-bias couplings may be used to prove simple, yet powerful, Poisson approximation results. Here we show how these assumptions may be relaxed, establishing explicit Poisson approximation bounds (depending on the first two moments only) for random variables which satisfy an approximate version of these monotonicity conditions. These are shown to be effective for models where an underlying random variable of interest is contaminated with noise. We also state explicit Poisson approximation bounds for sums of associated or negatively associated random variables. Applications are given to epidemic models, extremes, and random sampling. Finally, we also show how similar techniques may be used to relax the assumptions needed in a Poincar{\'{e}} inequality and in a normal approximation result.},
archivePrefix = {arXiv},
arxivId = {1706.04064},
author = {Daly, Fraser and Johnson, Oliver},
doi = {10.1017/jpr.2018.48},
eprint = {1706.04064},
file = {:home/wilfrid/Reading/Papers/D/DalyJohnson-2017.pdf:pdf},
issn = {00219002},
journal = {Journal of Applied Probability},
keywords = {(negative) association,Monotone coupling,Poincar{\'{e}} inequality,Poisson approximation,normal approximation size biasing,stochastic ordering},
number = {3},
pages = {742--759},
title = {{Relaxation of monotone coupling conditions: Poisson approximation and beyond}},
volume = {55},
year = {2018}
}
