year = {2008}
}
@article{HilarioUngaretti-2020,
abstract = {We study the independent alignment percolation model on {\$}\backslashmathbb{\{}Z{\}}{\^{}}d{\$} introduced by Beaton, Grimmett and Holmes [arXiv:1908.07203]. It is a model for random intersecting line segments defined as follows. First the sites of {\$}\backslashmathbb{\{}Z{\}}{\^{}}d{\$} are independently declared occupied with probability {\$}p{\$} and vacant otherwise. Conditional on the configuration of occupied vertices, consider the set of all line segments that are parallel to the coordinate axis, whose extremes are occupied vertices and that do not traverse any other occupied vertex. Declare independently the segments on this set open with probability {\$}\backslashlambda{\$} and closed otherwise. All the edges that lie on open segments are also declared open giving rise to a bond percolation model in {\$}\backslashmathbb{\{}Z{\}}{\^{}}d{\$}. We show that for any {\$}d \backslashgeq 2{\$} and {\$}p \backslashin (0,1]{\$} the critical value for {\$}\backslashlambda{\$} satisfies {\$}\backslashlambda{\_}c(p){\textless}1{\$} completing the proof that the phase transition is non-trivial over the whole interval {\$}(0,1]{\$}. We also show that the critical curve {\$}p \backslashmapsto \backslashlambda{\_}c(p){\$} is continuous at {\$}p=1{\$}, answering a question posed by the authors in [arXiv:1908.07203].},
archivePrefix = {arXiv},
arxivId = {2007.00539},
author = {Hil{\'{a}}rio, Marcelo and Ungaretti, Daniel},
eprint = {2007.00539},
file = {:home/wilfrid/Reading/Papers/H/HilarioUngaretti-2020.pdf:pdf},
journal = {arXiv},
keywords = {Stick percolation},
mendeley-tags = {Stick percolation},
pages = {13pp},
title = {{A note on the phase transition for independent alignment percolation}},
url = {http://arxiv.org/abs/2007.00539},
volume = {2007.00539},
year = {2020}
}
