@article{CaputoMartinelliSinclairStauffer-2012,
abstract = {The paper concerns lattice triangulations, that is, triangulations of the integer points in a polygon in ℝ2$\backslash$mathbb{\{}R{\}}{\^{}}{\{}2{\}} whose vertices are also integer points. Lattice triangulations have been studied extensively both as geometric objects in their own right and by virtue of applications in algebraic geometry. Our focus is on random triangulations in which a triangulation $\sigma$$\backslash$sigma has weight $\lambda$|$\sigma$|$\backslash$lambda{\^{}}{\{}|$\backslash$sigma|{\}}, where $\lambda$$\backslash$lambda is a positive real parameter, and |$\sigma$||$\backslash$sigma| is the total length of the edges in $\sigma$$\backslash$sigma. Empirically, this model exhibits a “phase transition” at $\lambda$=1$\backslash$lambda=1 (corresponding to the uniform distribution): for $\lambda${\textless}1$\backslash$lambda{\textless}1 distant edges behave essentially independently, while for $\lambda${\textgreater}1$\backslash$lambda{\textgreater}1 very large regions of aligned edges appear. We substantiate this picture as follows. For $\lambda${\textless}1$\backslash$lambda{\textless}1 sufficiently small, we show that correlations between edges decay exponentially with distance (suitably defined), and also that the Glauber dynamics (a local Markov chain based on flipping edges) is rapidly mixing (in time polynomial in the number of edges in the triangulation). This dynamics has been proposed by several authors as an algorithm for generating random triangulations. By contrast, for $\lambda${\textgreater}1$\backslash$lambda{\textgreater}1 we show that the mixing time is exponential. These are apparently the first rigorous quantitative results on the structure and dynamics of random lattice triangulations.},
annote = {"It is this non-locality that makes the application of established techniques very challenging here."

Small $\lambda$ case: "Our argument is reminiscent of classical “Peierls-type” arguments for the Ising model, which explains why we require $\lambda$ to be sufficiently small." Path coupling arguments used here.

Large $\lambda$ case: "by exhibiting an explicit bottleneck in the dynamics, specifically an initial configuration from which it takes a very long time to change the orientation of some long edge".

CF: Arak and Surgailis' idea of length-interacting polygonal Markov fields.},
author = {Caputo, Pietro and Martinelli, Fabio and Sinclair, Alistair and Stauffer, Alexandre},
doi = {10.1214/14-AAP1033},
file = {:home/wilfrid/Reading/Papers/C/CaputoMartinelliSinclairStauffer-2012.pdf:pdf},
isbn = {9781450320290},
issn = {10505164},
journal = {Annals of Applied Probability},
keywords = {+OngoingProjects,Glauber dynamics,Mixing times,Networks,Rapid mixing,SIRSN-RRF,Spatial mixing,Stochastic Geometry,Triangulations,networks},
mendeley-tags = {+OngoingProjects,Networks,SIRSN-RRF,Stochastic Geometry,networks},
number = {3},
pages = {1650--1685},
title = {{Random lattice triangulations: Structure and algorithms}},
url = {http://arxiv.org/abs/1211.1784},
volume = {25},
year = {2015}
