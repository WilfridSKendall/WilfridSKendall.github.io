@article{Kendall-2013b,
abstract = {The theory of random lines has a celebrated history, reaching back 300 years into the past to the work of Buffon, and forming a major part of the field of stochastic geometry. Recently it has found application in the derivation of surprising non-stochastic results concerning effective planar networks (Aldous and Kendall, 2008). The following is an account corresponding to three lectures on this material, including a very brief introduction to the relevant stochastic geometry, and also a description of some more recent work concerning flows in related networks (Kendall, 2008, 2011) as well as a rather curious random metric space.},
annote = {From Mathematical Reviews
This is a survey paper covering topics on Poisson random lines, based on lectures given by the author in 2012. 
   The first section, after some background on Poisson processes in general, introduces and gives definitions of Poisson random line processes in the plane. The second section covers work by Aldous and Kendall on the connection between random lines and transportation networks, where Poisson line processes can be used to construct transportation networks with low total network length. Finally, the third section surveys the development of the ideas of the previous section as the Poissonian city: a random metric space where the points are in a disc and the distances between them come from a Poisson line process. Reviewed by Jonathan Henry Jordan},
author = {Kendall, Wilfrid Stephen},
file = {:home/wilfrid/Reading/Papers/K/Kendall-2013b.pdf:pdf},
journal = {Markov Processes and Related Fields},
keywords = {Diaconis,SIRSN,Stochastic Geometry},
mendeley-tags = {Diaconis,SIRSN,Stochastic Geometry},
number = {1},
pages = {81--106},
title = {{Lines and networks}},
volume = {20},
year = {2014}
}
@article{GousseauRoueff-2018,
abstract = {In this work, we introduce a random field in view of natural image modeling, obtained as a limit of sequences of dead leaves models, when considering arbitrarily small or big objects. The dead leaves model, introduced by the Mathematical Morphology school, consists in the superposition of random closed sets, and enables to model the occlusion phenomena. When combined with specific sizes distributions for objects, they are known to provide adequate models for natural images. However this framework yields a small scales cutoff and a limit random field is introduced by letting this cutoff tend to zero. We first give a rigorous definition of the dead leaves model, and compute the probability that n compacts are included in distinct visible parts, which characterizes the model. Then, we derive our limit model and some of its property, and study its regularity.},
archivePrefix = {arXiv},
arxivId = {math/0312035},
author = {Gousseau, Yann and Roueff, Francois},
eprint = {0312035},
file = {:home/wilfrid/Reading/Papers/G/GousseauRoueff-2018.pdf:pdf},
journal = {arXiv},
pages = {38pp},
primaryClass = {math},
title = {{The dead leaves model: general results and limits at small scales}},
url = {http://arxiv.org/abs/math/0312035},
volume = {0312035},
year = {2018}
}
@article{Chiu-1995a,
abstract = {Johnson–Mehl tessellations can be considered as the results of spatial birth–growth processes. It is interesting to know when such a birth–growth process is completed within a bounded region. This paper deals with the limiting distributions of the time of completion for various models of Johnson–Mehl tessellations in ℝ d and k -dimensional sectional tessellations, where 1 ≦ k {\textless} d , by considering asymptotic coverage probabilities of the corresponding Boolean models. Random fractals as the results of birth–growth processes are also discussed in order to show that a birth–growth process does not necessarily lead to a Johnson–Mehl tessellation.},
