@article{CandelleroGilch-2013,
abstract = {Suppose we are given finitely generated groups
{\$}\Gamma{\_}1,...,\Gamma{\_}m{\$} equipped with irreducible random walks.
Thereby we assume that the expansions of the corresponding Green
functions at their radii of convergence contain only logarithmic
or algebraic terms as singular terms up to sufficiently large
order (except for some degenerate cases). We consider transient
random walks on the free product {\{}{\$}\Gamma{\_}1 \backslashast ...
\backslashast\Gamma{\_}m{\$}{\}} and give a complete classification of the possible
asymptotic behaviour of the corresponding {\$}n{\$}-step return
probabilities. They either inherit a law of the form
{\$}\backslashvarrho{\^{}}{\{}n\delta{\}} n{\^{}}{\{}-\lambda{\_}i{\}} \backslashlog{\^{}}{\{}\kappa{\_}i{\}}n{\$} from one of
the free factors {\$}\Gamma{\_}i{\$} or obey a {\$}\backslashvarrho{\^{}}{\{}n\delta{\}}
n{\^{}}{\{}-3/2{\}}{\$}-law, where {\$}\backslashvarrho{\textless}1{\$} is the corresponding spectral
radius and {\$}\delta{\$} is the period of the random walk. In addition,
we determine the full range of the asymptotic behaviour in the
case of nearest neighbour random walks on free products of the
form {\$}\backslashZ{\^{}}{\{}d{\_}1{\}}\backslashast ... \backslashast \backslashZ{\^{}}{\{}d{\_}m{\}}{\$}. Moreover, we characterize
the possible phase transitions of the non-exponential types
{\$}n{\^{}}{\{}-\lambda{\_}i{\}}\backslashlog{\^{}}{\{}\kappa{\_}i{\}}n{\$} in the case
{\$}\Gamma{\_}1\backslashast\Gamma{\_}2{\$}.},
annote = {This substantial paper (appearing in the famous RSA
journal) considers random walks on free products of groups. We
expect such walks to be transient (just think of random walk on
the free product of two copies of $\backslash$($\backslash$mathbb{\{}Z{\}}{\_}2$\backslash$)), so interest
centres on the behaviour of the return probabilities for large
time. A remarkable result by Cartwright showed that there are
varieties of asymptotic behaviour if the component groups are
cyclic of period $\backslash$(5$\backslash$) or more. It is shown in this paper that
modest and natural conditions on the spectral behaviour of the
individual random walks lead to a well-described variety of
possible asymptotic behaviours. Methods involve careful
asymptotics for Green functions based on recursive
characterizations. There is an impressive amount of detailed
analysis involved, and a satisfyingly clear result.},
archivePrefix = {arXiv},
arxivId = {arXiv:0909.1893v5},
author = {Candellero, Elisabetta and Gilch, Lorenz A},
eprint = {arXiv:0909.1893v5},
journal = {Random Structures {\&} Algorithms},
keywords = {Networks,Stochastic Geometry,asymptotic behaviour,free products,random walks,return probabilities},
mendeley-tags = {Networks,Stochastic Geometry},
number = {2},
pages = {15--181},
title = {{Phase transitions for random walk asymptotics on free products of groups}},
url = {http://onlinelibrary.wiley.com/doi/10.1002/rsa.20370/full},
volume = {40},
year = {2012}
}
