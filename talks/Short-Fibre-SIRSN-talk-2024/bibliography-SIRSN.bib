@article{Heinrich-1992b,
abstract = {We give a rigorous definition of germ-grain models ( g g m ' s ) which were introduced in [6 as at most countable unions Z = U,,, (X, + Z i ) of random closed sets {\{}Z,, i E N{\}} (called grains) in IW 3 translated by the atoms {\{}X,,i E N) (called germs) of a point process in R", and establish conditions under which the random set Z is a.s. closed. In case of i.i.d. grains we prove a continuity theorem for ggm's in terms of weak convergence. Further, we characterize ergodicity and (weak) mixing of stationary g g m ' s with a.s. compact grains by the corresponding properties of the underlying stationary point process. As a consequence we apply an ergodic theorem of Nguyen and Zessin [9] to spatial averages of certain geometric functionals of g g m ' s with a.s. compact convex grains.}},
annote = {The germ-grain models of the title are finite or countable unions Z=⋃i∈N(Xi+Zi) of random closed sets Zi, i∈N (the "grains'') in Rd translated by the atoms Xi, i∈N (the "germs'') of a point process in Rd. A crucial role in the theory of such models is played by the incidence function TZ(K)=P(Z∩K≠∅), defined for compact sets K. The paper deals mainly with models in which the grains are assigned independently to germs and are identically distributed. A condition is given in terms of the incidence function under which convergence in distribution of the grain and of the point process of germs implies convergence in distribution of the resulting germ-grain model. Ergodicity and mixing of a stationary germ-grain model are characterised in terms of its incidence function and it is shown that if a germ process is ergodic or mixing, then so is any germ-grain process resulting from it through the assignment of i.i.d. compact grains, provided its realisations are almost surely closed. The paper concludes with an ergodic theorem for the case of compact convex grains. Reviewed by F. Papangelou},
author = {Heinrich, L},
doi = {10.1080/02331889208802375},
file = {:home/wilfrid/Reading/Papers/H/Heinrich-1992b.pdf:pdf},
issn = {0233-1888},
journal = {Statistics},
month = {jan},
number = {3},
pages = {271--286},
title = {{On existence and mixing properties of germ-grain models}},
url = {https://www.tandfonline.com/doi/full/10.1080/02331889208802375},
volume = {23},
year = {1992}
}
