@article{Berestycki-2013,
abstract = {We construct the natural diffusion in the random geometry of planar Liouville quantum gravity. Formally, this is the Brownian motion in a domain DDD of the complex plane for which the Riemannian metric tensor at a point z in Dz in Dz$\backslash$in D is given by exp($\gamma$h(z))exp⁡($\gamma$h(z))$\backslash$exp($\backslash$gamma h(z)), appropriately normalised. Here hhh is an instance of the Gaussian free field on DDD and $\gamma$ in (0,2) is a parameter. We show that the process is almost surely continuous and enjoys certain conformal invariance properties. We also estimate the Hausdorff dimension of times that the diffusion spends in the thick points of the Gaussian free field, and show that it spends Lebesgue-almost all its time in the set of $\gamma$$\gamma$$\backslash$gamma-thick points, almost surely. Similar but deeper results have been independently and simultaneously proved by Garban, Rhodes and Vargas. , Nous construisons une diffusion naturelle associ{\'{e}}e {\^{e}} la g{\'{e}}om{\'{e}}trie al{\'{e}}atoire de la gravit{\'{e}} quantique de Liouville. Formellement, il s'ag{\^{i}}t d'un mouvement Brownien dans un domaine DDD du plan complexe, muni d'un tenseur de Riemann donn{\'{e}} par exp($\gamma$h(z))exp⁡($\gamma$h(z))$\backslash$exp($\backslash$gamma h(z)), correctement renomalis{\'{e}}. Ici hhh est une r{\'{e}}alisation du champ libre Gaussien sur DDD, et $\gamma$ in ]0,2[ est un param{\`{e}}tre. Il est montr{\'{e}} que ce processus est presque s{\^{u}}rement continu et poss{\`{e}}de certains propri{\'{e}}t{\'{e}}s d'invariance conforme. Une borne sur la dimension de Hausdorff des instants pass{\'{e}}s dans les points {\'{e}}pais du champ libre Gaussien est obtenue, qui montre que cette diffusion passe Lebesue-presque tout son temps dans les points $\gamma$$\gamma$$\backslash$gamma-{\'{e}}pais, presque s{\^{u}}rement. Des r{\'{e}}sultats semblables mais plus profonds ont {\'{e}}t{\'{e}} ind{\'{e}}pendemment et simultan{\'{e}}ment obtenus par Garban, Rhodes et Vargas.},
author = {Berestycki, Nathana{\"{e}}l},
doi = {10.1214/14-AIHP605},
issn = {0246-0203},
journal = {Annales de l'Institut Henri Poincar{\'{e}}, Probabilit{\'{e}}s et Statistiques},
