mendeley-tags = {Computer Science - Machine Learning,Computer Science - Neural and Evolutionary Computi,Folder - Networks,Statistics - Machine Learning,Stochastic Geometry},
month = {oct},
shorttitle = {One ticket to win them all},
title = {{One ticket to win them all: generalizing lottery ticket initializations across datasets and optimizers}},
