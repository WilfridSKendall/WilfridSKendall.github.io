journal = {Physical Review E},
keywords = {+OngoingProjects,SIRSN,SIRSN-RRF,physics.data-an,physics.soc-ph},
mendeley-tags = {+OngoingProjects,SIRSN,SIRSN-RRF},
number = {2},
