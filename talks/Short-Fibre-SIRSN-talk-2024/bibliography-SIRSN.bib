keywords = {Limit theorem,Markov jump process,Martingale inequality,OK corral gunfight model,Ordinary differential equation,Saddle fixed point},
number = {3},
pages = {1141--1171},
title = {{Convergence of Markov processes near saddle fixed points}},
volume = {35},
year = {2007}
