journal = {Journal of the Royal Statistical Society. Series B (Statistical Methodology)},
keywords = {+People,and phrases,evy measure,exchangeability,fc acknowledges the support,generalized gamma process,intra-,l,of the european commission,point process,random graphs,under the marie curie},
mendeley-tags = {+People},
number = {5},
pages = {1295--1366},
title = {{Sparse graphs using exchangeable random measures}},
